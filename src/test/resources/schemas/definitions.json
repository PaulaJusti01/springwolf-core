x-amazon-apigateway-integration:
  type: aws
  httpMethod: POST
  uri: arn:aws:apigateway:sa-east-1:runtime.sagemaker:path/endpoints/{modelId}/invocations
  credentials: arn:aws:iam::${iulpipes_deploy_aws_env_account}:role/${iulpipes_deploy_role}
  passthroughBehavior: "when_no_templates"
  requestTemplates:
    application/json: "$input.body"
  responses:
    default:
      statusCode: "200"
      responseTemplates:
        application/json: "$input.body"
