x-amazon-apigateway-integration:
  httpMethod: POST
  type: aws
  uri: arn:aws:apigateway:sa-east-1:runtime.sagemaker:path/endpoints/{modelId}/invocations
  passthroughBehavior: when_no_templates
  requestTemplates:
    application/json: |
      {
        "instances": [
          {
            "col0": $input.json('$.instances[0].col0'),
            "col1": $input.json('$.instances[0].col1'),
            "col2": $input.json('$.instances[0].col2'),
            "col3": $input.json('$.instances[0].col3'),
            "col4": $input.json('$.instances[0].col4'),
            "col5": $input.json('$.instances[0].col5'),
            "col6": $input.json('$.instances[0].col6'),
            "col7": $input.json('$.instances[0].col7'),
            "col8": $input.json('$.instances[0].col8'),
            "col9": $input.json('$.instances[0].col9')
          }
        ]
      }
  responses:
    default:
      statusCode: 200
      responseTemplates:
        application/json: |
          $input.json('$')
